        "sass": "^1.22.0",
        "sass-loader": "^7.3.1",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "babel-preset-env": "^1.7.0",
        "bcrypt": "^3.0.2",
        "github-syntax-light": "^0.5.0",
        "highlight.js": "^10.4.1",
        "laravel-mix-purgecss": "^4.0",
        "primer-markdown": "^4.0.0",
        "tailwindcss": "^0.5.2",
        "turbolinks": "^5.1.1"
